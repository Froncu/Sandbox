{
   "version" : 3,
   "configurePresets" : [
      {
         "name" : "base",
         "hidden" : true,
         "binaryDir" : "${sourceDir}/build/${presetName}",
         "cacheVariables" : {
            "CMAKE_TOOLCHAIN_FILE" : "${sourceDir}/fronge/vcpkg/scripts/buildsystems/vcpkg.cmake",
            "VCPKG_MANIFEST_DIR" : "${sourceDir}/fronge",
            "VCPKG_DEFAULT_STATIC" : true
         }
      },
      {
         "inherits" : "base",
         "name" : "debug",
         "cacheVariables" : {
            "CMAKE_BUILD_TYPE" : "Debug"
         }
      },
      {
         "inherits" : "base",
         "name" : "release",
         "cacheVariables" : {
            "CMAKE_BUILD_TYPE" : "Release"
         }
      }
   ]
}